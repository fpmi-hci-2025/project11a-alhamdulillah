@startuml
'Диаграмма классов — интеграция с доставкой'
class Заказ {
 -id : int
 -статус : String
 -методДоставки : String
}
class СервисДоставки {
 +получитьЗаказы()
 +отправитьЗаказ()
}
class Курьер {
 -id : int
 -имя : String
 -статус : String
 +взятьЗаказ()
 +доставитьЗаказ()
}
Заказ -->> СервисДоставки : передаётся в
СервисДоставки -->> Курьер : назначается
@enduml
