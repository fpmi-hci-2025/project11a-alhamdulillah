@startuml
'Диаграмма классов — основные сущности'
class Пользователь {
 -id : int
 -имя : String
 -email : String
 +сделатьЗаказ()
}
class Заказ {
 -id : int
 -идентификаторПользователя : int
 -статус : String
 -времяЗаказа : DateTime
 +получитьСтатус()
}
class Блюдо {
 -id : int
 -название : String
 -цена : double
 -категория : String
}
Пользователь -->> Заказ : делает
Заказ -->> Блюдо : включает
@enduml
